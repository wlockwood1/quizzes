<!--When form submitted, will see if value is equal to any of the solutions-->

<%= form_tag quiz_path(@quiz), :method => 'get', :id => 'guess' do %>
    <p>
      <%= text_field_tag :guess, params[:search], placeholder: "Guess here" %>
      <%= submit_tag "Guess answer", :name => nil %>
    </p>
<% end %>
